{
  "name": "landing-pad-ai-agents",
  "version": "1.0.0",
  "description": "AI agent framework for automating content operations at Landing Pad Digital",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "postinstall": "scripts/install-hooks.sh",
    "test": "jest",
    "test:unit": "jest --selectProjects=unit",
    "test:integration": "jest --selectProjects=integration",
    "test:api": "jest --selectProjects=api",
    "test:security": "jest --testPathPattern=__tests__/(unit|integration)/.*/(auth|security)",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "db:init": "node scripts/db-init.js",
    "lint": "eslint .",
    "lint:security": "eslint . --config .eslintrc.js --quiet --ignore-pattern 'node_modules/' --plugin security",
    "security:audit": "npm audit --production",
    "security:check": "node scripts/security-check.js",
    "security:scan": "npm run lint:security && npm run test:security && npm run security:audit && npm run security:check",
    "security:outdated": "npm outdated --depth=0",
    "security:license": "npx license-checker --summary --onlyAllow \"MIT;ISC;Apache-2.0;BSD-3-Clause;BSD-2-Clause;CC-BY-4.0;CC0-1.0\"",
    "agent:strategy": "node src/cli/agent-commands.js content_strategy",
    "agent:creation": "node src/cli/agent-commands.js content_creation",
    "agent:management": "node src/cli/agent-commands.js content_management",
    "agent:optimisation": "node src/cli/agent-commands.js optimisation",
    "agent:brand": "node src/cli/agent-commands.js brand_consistency",
    "monitor": "node examples/agent-health-integration.js",
    "migrate:status": "node src/cli/migration-commands.js status",
    "migrate:up": "node src/cli/migration-commands.js up",
    "migrate:down": "node src/cli/migration-commands.js down",
    "migrate:create": "node src/cli/migration-commands.js create",
    "ui:start": "react-scripts start",
    "ui:build": "react-scripts build",
    "ui:test": "react-scripts test",
    "ui:eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mctata/landing-pad-ai-agents.git"
  },
  "keywords": [
    "ai",
    "agents",
    "content",
    "marketing",
    "automation"
  ],
  "author": "Landing Pad Digital",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://github.com/mctata/landing-pad-ai-agents/issues"
  },
  "homepage": "https://github.com/mctata/landing-pad-ai-agents#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.15.0",
    "@atproto/api": "^0.12.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@wordpress/api-fetch": "^6.32.0",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1575.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "d3": "^7.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "fb": "^2.0.0",
    "form-data": "^4.0.0",
    "google-analytics-data": "^3.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "linkedin-api-v2": "^1.0.2",
    "migrate-mongo": "^12.1.3",
    "mongodb": "^6.4.0",
    "mongoose": "^8.12.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.9",
    "openai": "^4.29.0",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-scripts": "^5.0.1",
    "redis": "^4.6.13",
    "shopify-api-node": "^3.12.3",
    "speakeasy": "^2.0.0",
    "winston": "^3.12.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "license-checker": "^25.0.1",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
